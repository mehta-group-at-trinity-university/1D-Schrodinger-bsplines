Cell[TextData[{
 "The functions ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["u", "i"], "(", "R", ")"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " form a complete polynomial basis of degree ",
 Cell[BoxData[
  FormBox[
   RowBox[{"M", "+", "1"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 ", because any polynomial of degree ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[LessEqual]", " ", 
    RowBox[{"M", "+", "1"}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " can be expressed as the linear combination: ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["P", 
      RowBox[{"M", "+", "1"}]], "(", "R", ")"}], "=", 
    RowBox[{
     OverscriptBox[
      UnderscriptBox["\[Sum]", 
       RowBox[{"i", "=", "0"}]], 
      RowBox[{"M", "+", "1"}]], 
     RowBox[{
      RowBox[{
       SubscriptBox["u", "i"], "(", "R", ")"}], 
      RowBox[{
       SubscriptBox["P", 
        RowBox[{"M", "+", "1"}]], "(", 
       SubscriptBox["R", "i"], ")"}]}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 ".  Specializing this to the case ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["P", 
     RowBox[{"M", "+", "1"}]], "=", "1"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " gives ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     OverscriptBox[
      UnderscriptBox["\[Sum]", 
       RowBox[{"i", "=", "0"}]], 
      RowBox[{"M", "+", "1"}]], 
     RowBox[{
      SubscriptBox["u", "i"], "(", "R", ")"}]}], "=", "1"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 ".  Testing with ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["P", "2"], "(", "R", ")"}], "=", 
    RowBox[{"1", "+", "R", "+", 
     SuperscriptBox["R", "2"]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 ":\[LineSeparator]",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["P", "2"], "(", "R", ")"}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{
       FractionBox[
        RowBox[{"R", "-", 
         SubscriptBox["R", "1"]}], 
        RowBox[{
         SubscriptBox["R", "0"], "-", 
         SubscriptBox["R", "1"]}]], 
       FractionBox[
        RowBox[{"R", "-", 
         SubscriptBox["R", "2"]}], 
        RowBox[{
         SubscriptBox["R", "0"], "-", 
         SubscriptBox["R", "2"]}]], 
       RowBox[{"(", 
        RowBox[{"1", "+", 
         SubscriptBox["R", "0"], "+", 
         SuperscriptBox[
          SubscriptBox["R", "0"], "2"]}], ")"}]}], "+", 
      RowBox[{
       FractionBox[
        RowBox[{"R", "-", 
         SubscriptBox["R", "0"]}], 
        RowBox[{
         SubscriptBox["R", "1"], "-", 
         SubscriptBox["R", "0"]}]], 
       FractionBox[
        RowBox[{"R", "-", 
         SubscriptBox["R", "2"]}], 
        RowBox[{
         SubscriptBox["R", "1"], "-", 
         SubscriptBox["R", "2"]}]], 
       RowBox[{"(", 
        RowBox[{"1", "+", 
         SubscriptBox["R", "1"], "+", 
         SuperscriptBox[
          SubscriptBox["R", "1"], "2"]}], ")"}]}], "+", 
      RowBox[{
       FractionBox[
        RowBox[{"R", "-", 
         SubscriptBox["R", "0"]}], 
        RowBox[{
         SubscriptBox["R", "2"], "-", 
         SubscriptBox["R", "0"]}]], 
       FractionBox[
        RowBox[{"R", "-", 
         SubscriptBox["R", "1"]}], 
        RowBox[{
         SubscriptBox["R", "2"], "-", 
         SubscriptBox["R", "1"]}]], 
       RowBox[{"(", 
        RowBox[{"1", "+", 
         SubscriptBox["R", "2"], "+", 
         SuperscriptBox[
          SubscriptBox["R", "2"], "2"]}], ")"}]}]}], "=", 
     RowBox[{"1", "+", "R", "+", 
      SuperscriptBox["R", "2"]}]}]}], TraditionalForm]],
  CellChangeTimes->{{3.762010015095292*^9, 3.7620101721295567`*^9}}]
}], "Item1Numbered",
