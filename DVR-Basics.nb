Cell[BoxData[
 FormBox[
  RowBox[{
   OverscriptBox["P", "^"], "=", 
   RowBox[{
    OverscriptBox[
     UnderscriptBox["\[Sum]", 
      RowBox[{"j", "=", "1"}]], "n"], 
    RowBox[{
     TemplateBox[{SubscriptBox["\[Phi]", "j"]},
