 TextJustification->0.],

Cell["Calculate the weights, abscissa for the range 0,1", "Text",
 CellChangeTimes->{{3.76201937380667*^9, 3.762019391700808*^9}},
 TextAlignment->Left,
 TextJustification->0.],

Cell[BoxData[
 RowBox[{
  RowBox[{"M", "=", "50"}], ";", 
  RowBox[{"precision", "=", "20"}], ";", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{"absc", ",", "weights", ",", "errweights"}], "}"}], "=", 
   RowBox[{"NIntegrate`LobattoRuleData", "[", 
    RowBox[{
     RowBox[{"M", "+", "2"}], ",", "precision"}], "]"}]}], ";"}]], "Input",
 CellChangeTimes->{
  3.7620190582536573`*^9, {3.7620192709495087`*^9, 3.762019272348468*^9}, {
   3.7620196724520206`*^9, 3.762019672567423*^9}, {3.76202011217623*^9, 
   3.7620201314907293`*^9}},
 TextAlignment->Left,
 TextJustification->0.],

Cell[TextData[{
 "To re-scale these to an arbitrary range ",
 Cell[BoxData[
  FormBox[
   RowBox[{"x", "\[Element]", 
    RowBox[{"[", 
     RowBox[{"a", ",", "b"}], "]"}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " we multiply the abscissa by ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["x", "i"], "\[Rule]", 
    RowBox[{"a", "+", 
     RowBox[{
      SubscriptBox["x", "i"], " ", 
      RowBox[{"(", 
       RowBox[{"b", "-", "a"}], ")"}]}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"]
}], "Text",
 CellChangeTimes->{{3.7620159236025867`*^9, 3.762015998718416*^9}},
 TextAlignment->Left,
 TextJustification->0.],

Cell[BoxData[
 RowBox[{
  RowBox[{"a", "=", 
   RowBox[{"-", "10"}]}], ";", 
  RowBox[{"b", "=", "10"}], ";", 
  RowBox[{"gla", "=", 
   RowBox[{
    RowBox[{"absc", 
     RowBox[{"(", 
      RowBox[{"b", "-", "a"}], ")"}]}], "+", "a"}]}], ";"}]], "Input",
 CellChangeTimes->{
  3.7620192930627937`*^9, 3.762019405998124*^9, {3.7620201734425507`*^9, 
   3.762020175671772*^9}},
 TextAlignment->Left,
 TextJustification->0.],

Cell["Initialize the basis and plot it", "Text",
 CellChangeTimes->{{3.7620194182001677`*^9, 3.762019423467361*^9}}],

Cell[BoxData[{
 RowBox[{
  RowBox[{"basis", "=", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{"ugl", "[", 
      RowBox[{"gla", ",", "i", ",", "x"}], "]"}], ",", 
     RowBox[{"{", 
      RowBox[{"i", ",", "1", ",", 
       RowBox[{"M", "+", "2"}]}], "}"}]}], "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"Plot", "[", 
   RowBox[{
    RowBox[{"Evaluate", "[", "basis", "]"}], ",", 
    RowBox[{"{", 
     RowBox[{"x", ",", "a", ",", "b"}], "}"}], ",", 
    RowBox[{"PlotRange", "\[Rule]", "All"}], ",", 
    RowBox[{"Frame", "\[Rule]", "True"}]}], "]"}], ";"}]}], "Input",
 CellChangeTimes->{{3.762018946831622*^9, 3.762018971419956*^9}, {
   3.762019706528254*^9, 3.7620197086316147`*^9}, {3.762019925806753*^9, 
   3.7620199475118837`*^9}, 3.762020141452757*^9},
 TextAlignment->Left,
 TextJustification->0.],

Cell["\<\
Let\[CloseCurlyQuote]s see if we can now use this basis to calculate the \
matrix elements for the Harmonic oscillator:\
\>", "Text",
 CellChangeTimes->{{3.762016527711047*^9, 3.76201654802969*^9}},
 TextAlignment->Left,
 TextJustification->0.],

Cell[BoxData[
 FormBox[
  RowBox[{"T", "=", 
   RowBox[{
    FractionBox[
     RowBox[{"-", 
      SuperscriptBox["\[HBar]", "2"]}], 
     RowBox[{"2", "\[Mu]"}]], 
    FractionBox[
     SuperscriptBox["\[PartialD]", "2"], 
     RowBox[{"\[PartialD]", 
      SuperscriptBox["x", "2"]}]]}]}], TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{{3.762016551158897*^9, 3.7620165997505903`*^9}},
 TextAlignment->Left,
 TextJustification->0.],

Cell[BoxData[
 FormBox[
  RowBox[{"V", "=", 
   RowBox[{
    FractionBox["1", "2"], "\[Mu]", " ", 
    SuperscriptBox["\[Omega]", "2"], 
    SuperscriptBox["x", "2"]}]}], TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{{3.762016602884913*^9, 3.762016605957076*^9}},
 TextAlignment->Left,
 TextJustification->0.],

Cell["The matrix elements of the KE operator:", "Text",
 CellChangeTimes->{{3.762016640631023*^9, 3.762016646131133*^9}},
 TextAlignment->Left,
 TextJustification->0.],

Cell[BoxData[
 FormBox[
  RowBox[{
   SubscriptBox["T", 
    RowBox[{"i", ",", "j"}]], "=", 
   RowBox[{
    RowBox[{
     FractionBox[
      RowBox[{"-", 
       SuperscriptBox["\[HBar]", "2"]}], 
      RowBox[{"2", "\[Mu]"}]], 
     RowBox[{
      SubsuperscriptBox["\[Integral]", "a", "b"], 
      RowBox[{
       RowBox[{"\[DifferentialD]", "x"}], " ", 
       RowBox[{
        SubscriptBox["u", "i"], "(", "x", ")"}], 
       RowBox[{
        FractionBox[
         SuperscriptBox["\[PartialD]", "2"], 
         RowBox[{"\[PartialD]", 
          SuperscriptBox["x", "2"]}]], 
        RowBox[{
         SubscriptBox["u", "j"], "(", "x", ")"}]}]}]}]}], "=", 
    RowBox[{
     RowBox[{
      FractionBox[
       SuperscriptBox["\[HBar]", "2"], 
       RowBox[{"2", "\[Mu]"}]], 
      RowBox[{
       SubsuperscriptBox["\[Integral]", "a", "b"], 
       RowBox[{
        RowBox[{"\[DifferentialD]", "x"}], " ", 
        FractionBox[
         RowBox[{"\[PartialD]", 
          RowBox[{
           SubscriptBox["u", "i"], "(", "x", ")"}]}], 
         RowBox[{"\[PartialD]", "x"}]], 
        FractionBox[
         RowBox[{"\[PartialD]", 
          RowBox[{
           SubscriptBox["u", "j"], "(", "x", ")"}]}], 
         RowBox[{"\[PartialD]", "x"}]]}]}]}], "-", 
     SubsuperscriptBox[
      RowBox[{"(", 
       RowBox[{
        FractionBox[
         SuperscriptBox["\[HBar]", "2"], 
         RowBox[{"2", "\[Mu]"}]], 
        RowBox[{
         SubscriptBox["u", "i"], "(", "x", ")"}], 
        FractionBox[
         RowBox[{"\[PartialD]", 
          RowBox[{
           SubscriptBox["u", "j"], "(", "x", ")"}]}], 
         RowBox[{"\[PartialD]", "x"}]]}], ")"}], "a", "b"]}]}]}], 
  TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{{3.7620166499006023`*^9, 3.762016800181436*^9}, {
  3.76201684266782*^9, 3.762016848307652*^9}},
 TextAlignment->Left,
 TextJustification->0.],

Cell["\<\
The second \[OpenCurlyDoubleQuote]surface term vanishes when we are looking \
for bound states, so:\
\>", "Text",
 CellChangeTimes->{{3.762016804492097*^9, 3.7620168186840353`*^9}},
 TextAlignment->Left,
 TextJustification->0.],

Cell[BoxData[
 FormBox[
  RowBox[{
   SubscriptBox["T", 
    RowBox[{"i", ",", "j"}]], "=", 
   RowBox[{
    RowBox[{
     FractionBox[
      SuperscriptBox["\[HBar]", "2"], 
      RowBox[{"2", "\[Mu]"}]], 
     RowBox[{
      SubsuperscriptBox["\[Integral]", "a", "b"], 
      RowBox[{
       RowBox[{"\[DifferentialD]", "x"}], " ", 
       FractionBox[
        RowBox[{"\[PartialD]", 
         RowBox[{
          SubscriptBox["u", "i"], "(", "x", ")"}]}], 
        RowBox[{"\[PartialD]", "x"}]], 
       FractionBox[
        RowBox[{"\[PartialD]", 
         RowBox[{
          SubscriptBox["u", "j"], "(", "x", ")"}]}], 
        RowBox[{"\[PartialD]", "x"}]]}]}]}], "\[TildeTilde]", 
    RowBox[{
     FractionBox[
      SuperscriptBox["\[HBar]", "2"], 
      RowBox[{"2", "\[Mu]"}]], 
     RowBox[{
      UnderscriptBox["\[Sum]", "\[Alpha]"], 
      RowBox[{
       SubscriptBox["w", "\[Alpha]"], 
       FractionBox[
        RowBox[{"\[PartialD]", 
         RowBox[{
          SubscriptBox["u", "i"], "(", 
          SubscriptBox["x", "\[Alpha]"], ")"}]}], 
        RowBox[{"\[PartialD]", "x"}]], 
       FractionBox[
        RowBox[{"\[PartialD]", 
         RowBox[{
          SubscriptBox["u", "j"], "(", 
          SubscriptBox["x", "\[Alpha]"], ")"}]}], 
        RowBox[{"\[PartialD]", "x"}]]}]}]}]}]}], 
  TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{{3.7620168251005907`*^9, 3.7620168396200047`*^9}, {
  3.762016917677786*^9, 3.762016960469222*^9}},
 TextAlignment->Left,
 TextJustification->0.],

Cell["Calculate a matrix with the derivatives at the abscissa:", "Text",
 CellChangeTimes->{{3.76201943846137*^9, 3.7620194510344477`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"uglprime", "=", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"D", "[", 
       RowBox[{
        RowBox[{"basis", "[", 
         RowBox[{"[", "i", "]"}], "]"}], ",", "x"}], "]"}], "/.", 
      RowBox[{"x", "\[Rule]", 
       RowBox[{"gla", "[", 
        RowBox[{"[", "j", "]"}], "]"}]}]}], ",", 
     RowBox[{"{", 
      RowBox[{"i", ",", "1", ",", 
       RowBox[{"M", "+", "2"}]}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"j", ",", "1", ",", 
       RowBox[{"M", "+", "2"}]}], "}"}]}], "]"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.762017828112623*^9, 3.762017885591251*^9}, {
   3.7620185064380693`*^9, 3.762018512340768*^9}, 3.762018882502777*^9, {
   3.762019606477352*^9, 3.76201960820256*^9}},
 TextAlignment->Left,
 TextJustification->0.],

Cell["Calculate the matrix elements", "Text",
 CellChangeTimes->{{3.762019460191187*^9, 3.76201946620964*^9}}],

Cell[BoxData[{
 RowBox[{
  RowBox[{"T", "=", 
   RowBox[{
    FractionBox["1", "2"], 
    RowBox[{"Table", "[", 
     RowBox[{
      RowBox[{"Sum", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"weights", "[", 
          RowBox[{"[", "\[Alpha]", "]"}], "]"}], 
         RowBox[{"uglprime", "[", 
          RowBox[{"[", 
           RowBox[{"i", ",", "\[Alpha]"}], "]"}], "]"}], 
         RowBox[{"uglprime", "[", 
          RowBox[{"[", 
           RowBox[{"j", ",", "\[Alpha]"}], "]"}], "]"}]}], ",", 
        RowBox[{"{", 
         RowBox[{"\[Alpha]", ",", "1", ",", 
          RowBox[{"M", "+", "2"}]}], "}"}]}], "]"}], ",", 
      RowBox[{"{", 
       RowBox[{"i", ",", "2", ",", 
        RowBox[{"M", "+", "1"}]}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{"j", ",", "2", ",", 
        RowBox[{"M", "+", "1"}]}], "}"}]}], "]"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"S", "=", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"weights", "[", 
       RowBox[{"[", "i", "]"}], "]"}], 
      RowBox[{"KroneckerDelta", "[", 
       RowBox[{"i", ",", "j"}], "]"}]}], ",", 
     RowBox[{"{", 
      RowBox[{"i", ",", "2", ",", 
       RowBox[{"M", "+", "1"}]}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"j", ",", "2", ",", 
       RowBox[{"M", "+", "1"}]}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"V", "=", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"weights", "[", 
       RowBox[{"[", "i", "]"}], "]"}], 
      FractionBox["1", "2"], 
      SuperscriptBox[
       RowBox[{"gla", "[", 
        RowBox[{"[", "i", "]"}], "]"}], "2"], 
      RowBox[{"KroneckerDelta", "[", 
       RowBox[{"i", ",", "j"}], "]"}]}], ",", 
     RowBox[{"{", 
      RowBox[{"i", ",", "2", ",", 
       RowBox[{"M", "+", "1"}]}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"j", ",", "2", ",", 
       RowBox[{"M", "+", "1"}]}], "}"}]}], "]"}]}], ";"}]}], "Input",
 CellChangeTimes->{{3.762017919549004*^9, 3.7620179361323357`*^9}, {
   3.762017990792758*^9, 3.762018206776375*^9}, {3.7620183093194847`*^9, 
   3.762018310126265*^9}, {3.762018524550694*^9, 3.762018541628005*^9}, 
   3.762018888063002*^9, {3.762019128728187*^9, 3.7620191589313602`*^9}, {
   3.762019512158105*^9, 3.7620195356424026`*^9}, {3.762019613643231*^9, 
   3.76201965021731*^9}},
 TextAlignment->Left,
 TextJustification->0.],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"{", 
    RowBox[{"vals", ",", "vecs"}], "}"}], "=", 
   RowBox[{"Eigensystem", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"T", "+", "V"}], ",", "S"}], "}"}], ",", 
     RowBox[{"-", "1"}]}], "]"}]}], ";", "vals"}]], "Input",
 CellChangeTimes->{{3.762018324149641*^9, 3.762018352843102*^9}, {
   3.7620184273302307`*^9, 3.762018430679904*^9}, {3.7620188047979593`*^9, 
   3.762018804875414*^9}, {3.762019260572364*^9, 3.7620192608034067`*^9}, {
   3.762019480923232*^9, 3.7620194937060738`*^9}, {3.7620198796799726`*^9, 
   3.7620198833082733`*^9}, {3.7620200756789103`*^9, 3.762020086704978*^9}, 
   3.762020392474848*^9},
 TextAlignment->Left,
 TextJustification->0.],

Cell[BoxData[
 RowBox[{"{", "0.50000000009772543725673194818181172719`12.902711450879737", 
  "}"}]], "Output",
 CellChangeTimes->{{3.7620187936555557`*^9, 3.7620188320274057`*^9}, 
   3.7620189887819366`*^9, 3.762019075761826*^9, 3.762019162247787*^9, 
   3.7620192614105453`*^9, {3.762019472893552*^9, 3.762019530333293*^9}, 
   3.76201965152122*^9, 3.7620196860165787`*^9, 3.7620198839376383`*^9, {
   3.7620200761855803`*^9, 3.76202008720833*^9}, 3.762020124376869*^9, 
   3.762020157348987*^9, 3.762020195562643*^9, 3.762020393490409*^9},
