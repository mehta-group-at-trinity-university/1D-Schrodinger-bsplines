Cell[BoxData[
 RowBox[{
  RowBox[{"ugl", "[", 
   RowBox[{"absc_", ",", "i_", ",", "x_"}], "]"}], ":=", 
  RowBox[{
   RowBox[{"Product", "[", 
    RowBox[{
     FractionBox[
      RowBox[{"x", "-", 
       RowBox[{"absc", "[", 
        RowBox[{"[", "j", "]"}], "]"}]}], 
      RowBox[{
       RowBox[{"absc", "[", 
        RowBox[{"[", "i", "]"}], "]"}], "-", 
       RowBox[{"absc", "[", 
        RowBox[{"[", "j", "]"}], "]"}]}]], ",", 
     RowBox[{"{", 
      RowBox[{"j", ",", "1", ",", 
       RowBox[{"i", "-", "1"}]}], "}"}]}], "]"}], 
   RowBox[{"Product", "[", 
    RowBox[{
     FractionBox[
      RowBox[{"x", "-", 
       RowBox[{"absc", "[", 
        RowBox[{"[", "j", "]"}], "]"}]}], 
      RowBox[{
       RowBox[{"absc", "[", 
        RowBox[{"[", "i", "]"}], "]"}], "-", 
       RowBox[{"absc", "[", 
        RowBox[{"[", "j", "]"}], "]"}]}]], ",", 
     RowBox[{"{", 
      RowBox[{"j", ",", 
       RowBox[{"i", "+", "1"}], ",", 
       RowBox[{"M", "+", "2"}]}], "}"}]}], "]"}]}]}]], "Input",
 CellChangeTimes->{{3.762013570566304*^9, 3.762013662264132*^9}, {
  3.762013709989736*^9, 3.76201372720238*^9}, {3.76201891027323*^9, 
